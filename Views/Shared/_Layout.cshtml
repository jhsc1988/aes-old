        <script src="~/datatables-compact.min.js"></script>*@


    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/v/dt/dt-1.10.23/kt-2.6.1/r-2.2.7/sc-2.0.3/datatables.min.js"></script>
    <script src="//cdn.datatables.net/plug-ins/1.10.19/dataRender/datetime.js"></script>

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.10.23/kt-2.6.1/r-2.2.7/sc-2.0.3/datatables.min.css" />





    <!--<script src="~/datatables/datatables.min.js"></script>
    <link href="~/datatables/datatables.min.css" rel="stylesheet" />
    <!--<link href="~/datatables/datatables.min.css" rel="stylesheet" />-->
    <!--<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.1/semantic.min.css" />
        <link rel="stylesheet" href="https://cdn.datatables.net/1.10.23/css/dataTables.semanticui.min.css" />

        <script src="https://cdn.datatables.net/1.10.23/js/dataTables.semanticui.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.1/semantic.min.js"></script>-->
    <script src="https://cdn.datatables.net/plug-ins/1.10.22/dataRender/ellipsis.js"></script>




</head>
                <body>
                    <header>
                        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
                            <div class="container">
                                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">aes</a>
                                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                                        aria-expanded="false" aria-label="Toggle navigation">
                                    <span class="navbar-toggler-icon"></span>
                                </button>
                                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                                    <ul class="navbar-nav flex-grow-1">
                                        <li class="nav-item">
                                            <a class="nav-link text-dark" asp-area="" asp-controller="Stanovi" asp-action="">Stanovi</a>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Kupci<span class="caret"></span></a>
                                            <ul class="dropdown-menu">
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="ElektraKupci" asp-action="">Kupci - Elektra</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="OdsKupci" asp-action="">Kupci - ODS</a>
                                                </li>
                                                <li class="dropdown-divider"></li>
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="Ods" asp-action="">ODS - mjerna mjesta</a>
                                                </li>
                                            </ul>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Računi<span class="caret"></span></a>
                                            <ul class="dropdown-menu">
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="RacuniElektra" asp-action="">Elektra - obračuni</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="RacuniElektraRate" asp-action="">Elektra - rate</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="RacunElektraIzvrsenjaUsluges" asp-action="">Elektra - izvršenja usluge</a>
                                                </li>
                                                <li class="dropdown-divider"></li>
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="RacunOdsIzvrsenjaUsluge" asp-action="">ODS - izvršenja usluge</a>
                                                </li>
                                                <li class="dropdown-divider"></li>
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="RacuniHolding" asp-action="">Holding</a>
                                                </li>
                                                <li class="dropdown-divider"></li>
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="RacunElektraObracuniPotrosnje" asp-action="">Elektra - obračuni potrošnje</a>
                                                </li>
                                            </ul>
                                        </li>
                                        <li class="dropdown">
                                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Predmeti<span class="caret"></span></a>
                                            <ul class="dropdown-menu">
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="Dopisi" asp-action="">Dopisi</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link text-dark" asp-area="" asp-controller="Predmeti" asp-action="">Predmeti</a>
                                                </li>
                                            </ul>
                                        </li>
                                    </ul>
                                    <partial name="_LoginPartial" />
                                </div>
                            </div>
                        </nav>
                    </header>
                    <div class="container">
                        <main role="main" class="pb-3">
                            @RenderBody()
                        </main>
                    </div>

                    <footer class="border-top footer text-muted">
                        <div class="container">
                            &copy; 2021 - aes - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </div>
                    </footer>
                    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
                    <script src="~/js/site.js" asp-append-version="true"></script>
                    @await RenderSectionAsync("Scripts", required: false)
                </body>
                </html>
